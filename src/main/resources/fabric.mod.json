{
  "schemaVersion": 1,
  "id": "neospawnpoints",
  "version": "${version}",

  "name": "NeoSpawnPoints",
  "description": "A simple multiple-spawn manager plugin based on weight.",
  "authors": [
    "Neovitalism"
  ],
  "contact": {
    "homepage": "https://discord.gg/fQXCccxMrD"
  },

  "license": "MIT",
  "icon": "assets/neospawnpoints/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.neovitalism.neospawnpoints.NeoSpawnPoints"
    ]
  },

  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "~${minecraft_version}",
    "java": ">=21",
    "neoapi": ">=${neo_api_version}"
  },

  "mixins": [
    "neospawnpoints.mixins.json"
  ],

  "suggests": {
    "luckperms": ">=5.4.139"
  }
}